{
  "manifest_version": "2",
  "api_version": "1",
  "name": "Switcher",
  "description": "Cycles through active applications",
  "developer_name": "Artur Mroz",
  "icon": "images/switch.png",
  "options": {
    "query_debounce": 0.1
  },
  "preferences": [{
      "id": "switch_kw",
      "type": "keyword",
      "name": "Switch keyword",
      "default_value": "s"
    },
    {
      "id": "workspace_indicator",
      "type": "select",
      "name": "Workspace indicator",
      "description": "Whether to display or hide the workspace indicator at the begining of the description",
      "default_value": "true",
      "options": [{
          "value": "false",
          "text": "Hide indicator"
        },
        {
          "value": "true",
          "text": "Show indicator"
        }
      ]
    }
  ]
}